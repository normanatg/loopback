{
  "name": "Instructor",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "string",
      "required": true
    },
    "bio": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "courses": {
      "type": "hasMany",
      "model": "courses",
      "foreignKey": "IdInstructor",
      "through": "courses"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
